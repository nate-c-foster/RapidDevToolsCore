{
  "custom": {},
  "params": {
    "label": "SCADA",
    "type": "",
    "value": true
  },
  "propConfig": {
    "params.label": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.type": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 30,
      "width": 345
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "script": "\t\n\tpayload \u003d { \"type\": self.view.params.type,\n\t\t\t\t\"label\": self.view.params.label,\n\t\t\t\t\"value\": self.props.selected}\n\t\n\tsystem.perspective.sendMessage(\"UpdateBackupOptions\",payload)"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Checkbox"
        },
        "position": {
          "basis": "30px",
          "shrink": 0
        },
        "propConfig": {
          "props.selected": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.value"
              },
              "type": "property"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.label"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "RapidDev/Checkbox",
            "fontSize": "14px"
          },
          "uncheckedIcon": {
            "color": {
              "enabled": "var(--neutral-50)"
            }
          }
        },
        "type": "ia.input.checkbox"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "justify": "center"
    },
    "type": "ia.container.flex"
  }
}